<extentreports>
  <configuration>
    <!-- report theme -->
    <!-- standard, dark -->
    <theme>standard</theme>
  
    <!-- document encoding -->
    <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>
    
    <!-- protocol for script and stylesheets -->
    <!-- defaults to https -->
    <protocol>https</protocol>
    
    <!-- title of the document -->
    <documentTitle>InsuranceGuru</documentTitle>
    
    <!-- report name - displayed at top-nav -->
    <reportName></reportName>
    
    <theme>light</theme>
    
    <!-- report headline - displayed at top-nav, after reportHeadline -->
    <reportHeadline>InsuranceGuru Version 0.0.1</reportHeadline>
    
    <!-- global date format override -->
    <!-- defaults to yyyy-MM-dd -->
    <dateFormat>yyyy-MM-dd</dateFormat>
    
    <!-- global time format override -->
    <!-- defaults to HH:mm:ss -->
    <timeFormat>HH:mm:ss</timeFormat>
    
    <!-- custom javascript -->
    <scripts>

		<![CDATA[

			$(document).ready(function() {


			$(".logo-content").text('InsuranceGuru');
			});

				]]>

		</scripts>
    
    <!-- custom styles -->
    <styles>

	<![CDATA[

.report-name > img { float:none;height: 90%;margin-left: 0px;margin-top: 2px;width: auto; }

]]>

</styles>
  </configuration>
</extentreports>